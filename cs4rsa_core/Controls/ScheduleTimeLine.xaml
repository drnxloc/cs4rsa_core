<UserControl x:Class="cs4rsa_core.Controls.ScheduleTimeLine"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:cs4rsa_core.Controls"
             mc:Ignorable="d" 
             d:DataContext="{d:DesignInstance Type=local:ScheduleTimeLine}"
             d:DesignHeight="450" d:DesignWidth="800">
    <StackPanel Orientation="Horizontal" VerticalAlignment="Center">
        <Label x:Name="LabelControl"  FontSize="10" Content="{Binding Time}"></Label>
        <Line x:Name="LineControl" Stroke="#dfe6e9" StrokeThickness="1"></Line>
    </StackPanel>
</UserControl>
