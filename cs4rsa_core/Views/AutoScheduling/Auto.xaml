<base:ScreenAbstract
  x:Class="Cs4rsa.Views.AutoScheduling.Auto"
  xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
  xmlns:base="clr-namespace:Cs4rsa.BaseClasses"
  xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
  xmlns:local="clr-namespace:Cs4rsa.Views.AutoScheduling"
  xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
  mc:Ignorable="d"
  Loaded="ScreenAbstract_Loaded"
  >
  <Grid>
    <Grid.ColumnDefinitions>
      <ColumnDefinition Width="3*" />
      <ColumnDefinition />
    </Grid.ColumnDefinitions>
    <local:ProgramTree x:Name="ProgramTreeComponent" Grid.Column="0" Margin="5,5,5,5" />
    <local:Result Grid.Column="1" Margin="0,5,5,5" />
  </Grid>
</base:ScreenAbstract>
